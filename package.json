{
  "name": "mosaico",
  "version": "0.17.3",
  "description": "Mosaico - OpenSource Email Template Editor",
  "main": [
    "./src/js/app.js",
	"./src/js/template-translation-worker.js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:jhorowitz-firedrum/mosaico.git"
  },
  "browser": {
    "jquery.ui.widget": "jquery-ui/ui/widget.js",
    "jquery-ui": "jquery-ui/dist/jquery-ui.js"
  },
  "browserify": {
    "transform": [
      "aliasify",
      "browserify-shim",
      "browserify-versionify",
      "uglifyify"
    ]
  },
  "aliasify": {
    "aliases": {
      "blueimp-canvas-to-blob": "blueimp-canvas-to-blob/js/canvas-to-blob.js",
      "blueimp-file-upload-iframe-transport": "blueimp-file-upload/js/jquery.iframe-transport.js",
      "blueimp-file-upload-process": "blueimp-file-upload/js/jquery.fileupload-process.js",
      "blueimp-file-upload-image": "blueimp-file-upload/js/jquery.fileupload-image.js",
      "blueimp-file-upload-validate": "blueimp-file-upload/js/jquery.fileupload-validate.js",
      "console": "console-browserify",
      "jquery-textselection": "jquery-textselection/dist/jquery.textselection.js",
      "jquery-ui/draggable": "jquery-ui/ui/widgets/draggable.js",
      "jquery-ui/droppable": "jquery-ui/ui/widgets/droppable.js",
      "jquery-ui/sortable": "jquery-ui/ui/widgets/sortable.js",
      "jquery-ui/spinner": "jquery-ui/ui/widgets/spinner.js",
      "jquery-ui/tabs": "jquery-ui/ui/widgets/tabs.js",
      "jquery.ui.widget": "jquery-ui/ui/widget.js",
      "jquery-ui": "jquery-ui/dist/jquery-ui.js",
      "jqueryui-touch-punch": "jquery-ui-touch-punch",
      "knockout-jqueryui": "knockout-jqueryui/dist/knockout-jqueryui.js",
      "knockoutjs-reactor": "ko-reactor/dist/ko-reactor.js",
      "tinycolor": "tinycolor2"
    }
  },
  "browserify-shim": {
    "jquery": {
	  "exports": "$",
	  "depends": ["jsdom-global"]
	},
    "jquery-ui": {
	  "depends": ["jquery:jQuery"]
	},
	"jquery-ui/draggable": {
	  "depends": ["jquery:jQuery"]
	},
	"jquery-ui/droppable": {
	  "depends": ["jquery:jQuery"]
	},
	"jquery-ui/sortable": {
	  "depends": ["jquery:jQuery"]
	},
	"jquery-ui/spinner": {
	  "depends": ["jquery:jQuery"]
	},
	"jquery-ui/tabs": {
	  "depends": ["jquery:jQuery"]
	},
    "knockout-jqueryui": {
		"exports": "kojqui",
		"depends": ["jquery:jQuery", "knockout:ko" ]
	},
	"blueimp-load-image": {
		"depends": ["blueimp-load-image-orientation"]
	},
    "tinymce": "global:tinymce"
  },
  "dependencies": {
    "blueimp-canvas-to-blob": "3.5.0",
    "blueimp-load-image": "2.12.2",
    "blueimp-file-upload": "9.21.0",
    "console-browserify": "^1.1.0",
    "default-passive-events": "^1.0.7",
    "downloadjs": "~1.4.6",
    "emojionearea": "jhorowitz-firedrum/emojionearea",
    "evol-colorpicker": "^3.3.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "jquery-migrate": "^3.0.1",
    "jquery-textselection": "jhorowitz-firedrum/jquery-textselection",
    "jquery-ui": "jhorowitz-firedrum/jquery-ui",
    "jquery-ui-touch-punch": "^0.2.3",
    "jquery.iframe-transport": "^1.0.0",
    "jsdom": "^11.8.0",
    "jsep": "^0.3.3",
    "juice": "^4.2.3",
    "knockout": "jhorowitz-firedrum/knockout",
    "knockout-jqueryui": "jhorowitz-firedrum/knockout-jqueryui",
    "knockout-sortable": "^1.1.0",
    "ko-reactor": "jhorowitz-firedrum/knockoutjs-reactor",
    "mensch": "^0.3.3",
    "slick": "1.12.2",
    "tinycolor2": "1.4.1",
    "tinymce": "4.5.8",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "aliasify": "2.1.0",
    "async": "2.6.0",
    "autoprefixer": "8.1.0",
    "body-parser": "1.18.2",
    "browserify": "16.1.1",
    "browserify-incremental": "3.1.1",
    "browserify-shim": "3.8.14",
    "browserify-versionify": "^1.0.6",
    "cheerio": "1.0.0-rc.2",
    "connect-livereload": "0.6.0",
    "csswring": "6.0.3",
    "express": "4.16.2",
    "gm": "1.23.1",
    "grunt": "1.0.2",
    "grunt-browserify": "5.2.0",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-cssmin": "2.2.1",
    "grunt-contrib-jshint": "1.1.0",
    "grunt-contrib-less": "1.4.1",
    "grunt-contrib-uglify": "3.3.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-exorcise": "2.1.1",
    "grunt-express-server": "0.5.4",
    "grunt-google-fonts": "^0.5.0",
    "grunt-jasmine-node-coverage": "2.0.1",
    "grunt-keepalive": "^1.0.0",
    "grunt-newer": "^1.2.0",
    "grunt-parallel": "0.5.1",
    "grunt-postcss": "0.9.0",
    "grunt-release": "0.13.1",
    "jasmine-core": "3.1.0",
    "jquery-file-upload-middleware": "0.1.8",
    "jsdom-global": "jhorowitz-firedrum/jsdom-global",
    "jshint-stylish": "2.2.1",
    "license-checker": "16.0.0",
    "load-grunt-tasks": "3.5.2",
    "lodash": "4.17.5",
    "mockery": "2.1.0",
    "nodemailer": "4.6.2",
    "uglifyify": "4.0.5"
  },
  "author": "Stefano Bagnara",
  "contributors": [
    {
        "name": "Jonathan Horowitz",
        "email": "jhorowitz@firedrum.com"
    }
  ],
  "license": "GPL-3.0",
  "licenseurl": "https://www.gnu.org/licenses/gpl-3.0.en.html",
  "scripts": {
    "test": "grunt jasmine_node"
  },
  "engine": "node >= 6.0.0"
}
